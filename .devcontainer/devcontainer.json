{
    "name": "E Raven",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "customizations": {
      "vscode": {
        "settings": {
          "python.defaultInterpreterPath": "/usr/local/bin/python"
        },
        "extensions": [
          "ms-python.python",
          "ms-azuretools.vscode-docker"
        ]
      }
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "forwardPorts": [8000],
    "remoteUser": "vscode"
  }
  